#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/movie/server/node_modules/.pnpm/@mapbox/node-pre-gyp@1.0.5/node_modules/@mapbox/node-pre-gyp/bin/node-pre-gyp/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/movie/server/node_modules/.pnpm/@mapbox/node-pre-gyp@1.0.5/node_modules/@mapbox/node-pre-gyp/bin/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/movie/server/node_modules/.pnpm/@mapbox/node-pre-gyp@1.0.5/node_modules/@mapbox/node-pre-gyp/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/movie/server/node_modules/.pnpm/@mapbox/node-pre-gyp@1.0.5/node_modules/@mapbox/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/movie/server/node_modules/.pnpm/@mapbox/node-pre-gyp@1.0.5/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/movie/server/node_modules/.pnpm/@mapbox/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/movie/server/node_modules/.pnpm/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/movie/server/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/movie/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/node_modules:/mnt/c/Users/hp/Documents/bridge/node_modules:/mnt/c/Users/hp/Documents/node_modules:/mnt/c/Users/hp/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/movie/server/node_modules/.pnpm/node_modules/@mapbox/node-pre-gyp/bin/node-pre-gyp/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/movie/server/node_modules/.pnpm/node_modules/@mapbox/node-pre-gyp/bin/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/movie/server/node_modules/.pnpm/node_modules/@mapbox/node-pre-gyp/node_modules:/mnt/c/Users/hp/Documents/bridge/2projects/js/nodeMongo/movie/server/node_modules/.pnpm/node_modules/@mapbox/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@mapbox/node-pre-gyp/bin/node-pre-gyp" "$@"
else
  exec node  "$basedir/../@mapbox/node-pre-gyp/bin/node-pre-gyp" "$@"
fi
